{
  "name": "@shaderfrog/glsl-parser",
  "engines": {
    "node": ">=16"
  },
  "version": "2.0.0",
  "description": "A GLSL ES 1.0 and 3.0 parser and preprocessor that can preserve whitespace and comments",
  "scripts": {
    "build": "./build.sh",
    "watch-test": "jest --watch",
    "test": "jest --colors"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ast": {
      "types": "./dist/ast/index.d.ts",
      "default": "./dist/ast/index.js"
    },
    "./utils": {
      "types": "./dist/parser/utils.d.ts",
      "default": "./dist/parser/utils.js"
    },
    "./preprocessor": {
      "types": "./dist/preprocessor/index.d.ts",
      "default": "./dist/preprocessor/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaderFrog/glsl-parser.git"
  },
  "keywords": [
    "glsl",
    "parser",
    "shaderfrog"
  ],
  "author": "Andrew Ray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShaderFrog/glsl-parser/issues"
  },
  "homepage": "https://github.com/ShaderFrog/glsl-parser#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "peggy": "^3.0.2",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  }
}